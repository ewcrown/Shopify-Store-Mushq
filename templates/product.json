        "custom_liquid_zWzCy6": {
          "type": "custom_liquid",
          "settings": {
            "custom_liquid": "{% if product.tags contains 'ss-chiffon' or product.tags contains 'un-wed' or product.tags contains 'Inception' or product.tags contains 'add-lining' %}\n<div class=\"product-card-ai\">\n  <!-- Sleeve Lining Option -->\n  <div class=\"product-card-ai__option\">\n    <input \n      type=\"checkbox\" \n      id=\"sleeve-lining-checkbox\" \n      class=\"product-card-ai__input sleeve-lining-checkbox\" \n      value=\"false\"\n    />\n    <label for=\"sleeve-lining-checkbox\" class=\"product-card-ai__label\">\n      Add sleeve lining\n    </label>\n  </div>\n</div>\n{% endif %}\n\n{% if product.tags contains 'embroidered-sleeves' %}\n<div class=\"product-card-ai\">\n  <!-- Embroidered Sleeves Option -->\n  <div class=\"product-card-ai__option\">\n    <input \n      type=\"checkbox\" \n      id=\"embroidered-sleeves-checkbox\" \n      class=\"product-card-ai__input embroidered-sleeves-checkbox\" \n      value=\"false\"\n    />\n    <label for=\"embroidered-sleeves-checkbox\" class=\"product-card-ai__label\">\n      Add embroidered sleeves\n    </label>\n  </div>\n</div>\n{% endif %}\n\n<style>\n.product-card-ai { \n  font-family: system-ui, -apple-system, \"Segoe UI\", Roboto, Arial; \n  max-width:380px; \n  margin-bottom: 1rem;\n}\n.product-card-ai__title { \n  margin: 0 0 0.75rem; \n  font-size: 1.1rem; \n}\n.product-card-ai__option { \n  display:block; \n  border:1px solid #e0e0e0; \n  padding: 12px 16px;\n  border-radius:8px; \n  background: #f9f9f9;\n}\n.product-card-ai__input { \n  position: absolute; \n  left: -9999px; \n} /* visually hide native checkbox */\n.product-card-ai__label { \n  display:flex; \n  align-items:center; \n  gap:0.5rem; \n  cursor:pointer; \n  user-select:none; \n  position:relative; \n  padding-left:32px;\n  font-size: 0.95rem;\n  color: #333;\n}\n\n/* custom checkbox square */\n.product-card-ai__label::before{\n  content: \"\";\n  position: absolute;\n  left: 0;\n  width: 18px;\n  height: 18px;\n  border: 2px solid #777;\n  border-radius: 4px;\n  background: white;\n  box-sizing: border-box;\n  transition: all 0.2s ease;\n}\n\n/* checked state indicator */\n.product-card-ai__input:checked + .product-card-ai__label::before{\n  background: linear-gradient(135deg, #000, #333);\n  border-color: #000;\n  box-shadow: inset 0 0 0 2px white;\n}\n\n/* hover effect */\n.product-card-ai__label:hover::before {\n  border-color: #333;\n}\n</style>\n\n<script>\ndocument.addEventListener('DOMContentLoaded', function() {\n  // Handle sleeve lining checkbox\n  const sleeveLiningCheckbox = document.getElementById('sleeve-lining-checkbox');\n  const sleeveLiningHidden = document.querySelector('input[name=\"properties[sleeve lining]\"]');\n  \n  if (sleeveLiningCheckbox && sleeveLiningHidden) {\n    // Update hidden field on change\n    sleeveLiningCheckbox.addEventListener('change', function() {\n      sleeveLiningHidden.value = this.checked ? 'Yes' : 'No';\n      console.log('Sleeve lining:', sleeveLiningHidden.value);\n    });\n  }\n  \n  // Handle embroidered sleeves checkbox\n  const embroideredSleevesCheckbox = document.getElementById('embroidered-sleeves-checkbox');\n  const embroideredSleevesHidden = document.getElementById('embroidered-sleeves');\n  \n  if (embroideredSleevesCheckbox && embroideredSleevesHidden) {\n    // Update hidden field on change\n    embroideredSleevesCheckbox.addEventListener('change', function() {\n      embroideredSleevesHidden.value = this.checked ? 'Yes' : 'No';\n      console.log('Embroidered sleeves:', embroideredSleevesHidden.value);\n    });\n  }\n});\n</script>"
          }
