        "custom_liquid_Y3enhU": {
          "type": "custom_liquid",
          "settings": {
            "custom_liquid": "{% assign TDT = blank %}\n{% for tag in product.tags %}\n  {% if tag contains 'TDT' %}\n    {% assign TDT = tag | split: '--' %}\n    {% assign TDT = TDT[1] | strip %}\n  {% endif %}\n{% endfor %}\n\n{% if TDT %}\n  <span style=\"display:inline\">Note: </span><div class=\"ew-estimated-delivery\">\n    {{ TDT }}\n  </div>\n{% endif %}\n\n<script>\n(function () {\n  const DEBUG = true;\n  const log = (...a) => DEBUG && console.log('[ew-br]', ...a);\n\n  // Cache for performance\n  let cachedParts = null;\n  let sourceElement = null;\n\n  function getSourceElement() {\n    if (!sourceElement) {\n      sourceElement = document.querySelector('.ew-estimated-delivery');\n    }\n    return sourceElement;\n  }\n\n  function getSelectedSize() {\n    const checked = document.querySelector('input[name=\"Size\"]:checked');\n    return checked ? checked.value.trim() : '';\n  }\n\n  function parseBrContent(html) {\n    if (cachedParts) return cachedParts;\n    \n    // Split by <br> and clean HTML tags\n    const parts = html\n      .split(/<br\\s*\\/?>/gi)\n      .map(part => {\n        // Remove any HTML tags but keep the text\n        const tempDiv = document.createElement('div');\n        tempDiv.innerHTML = part;\n        return tempDiv.textContent || tempDiv.innerText || '';\n      })\n      .map(text => text.trim())\n      .filter(Boolean);\n    \n    cachedParts = parts;\n    log('Parsed parts:', parts);\n    return parts;\n  }\n\n  function updateMessage(reason = 'unknown') {\n    const source = getSourceElement();\n    if (!source) {\n      log('Source element not found');\n      return;\n    }\n\n    // Store original HTML on first access\n    if (!source.dataset.originalHtml) {\n      source.dataset.originalHtml = source.innerHTML;\n      log('Original HTML stored:', source.dataset.originalHtml);\n    }\n\n    const parts = parseBrContent(source.dataset.originalHtml);\n    if (parts.length === 0) return;\n\n    const size = getSelectedSize();\n    const isUnstitched = size === 'Unstitched';\n    \n    // Use index 0 for unstitched, index 1 for stitched\n    // If only one part exists, use it for both\n    let message = '';\n    if (isUnstitched && parts.length > 0) {\n      message = parts[0]; // First part before <br>\n    } else if (parts.length > 1) {\n      message = parts[1]; // Second part after <br>\n    } else {\n      message = parts[0]; // Fallback to first part\n    }\n\n    // Update the element's text content\n    source.textContent = message;\n    \n    log('Updated:', { reason, size, isUnstitched, messageLength: message.length });\n  }\n\n  // Debounce function for performance\n  let updateTimeout;\n  function debouncedUpdate(reason) {\n    clearTimeout(updateTimeout);\n    updateTimeout = setTimeout(() => updateMessage(reason), 50);\n  }\n\n  // Event handler for size changes\n  function handleSizeChange(e) {\n    if (e.target.name === 'Size') {\n      debouncedUpdate('change');\n    }\n  }\n\n  // Event handler for label clicks\n  function handleLabelClick(e) {\n    const label = e.target.closest('.product-form__label');\n    if (!label) return;\n\n    const radioId = label.getAttribute('for');\n    if (!radioId) return;\n\n    const radio = document.getElementById(radioId);\n    if (radio && radio.name === 'Size') {\n      // Small delay to ensure radio is checked\n      setTimeout(() => debouncedUpdate('click'), 10);\n    }\n  }\n\n  // Initialize\n  function init() {\n    log('Initializing...');\n    \n    // Initial update on DOM ready\n    if (document.readyState === 'loading') {\n      document.addEventListener('DOMContentLoaded', () => debouncedUpdate('DOMContentLoaded'));\n    } else {\n      debouncedUpdate('DOMContentLoaded');\n    }\n\n    // Set up event listeners\n    document.addEventListener('change', handleSizeChange);\n    document.addEventListener('click', handleLabelClick);\n\n    // Optional: MutationObserver for dynamic content changes\n    if (typeof MutationObserver !== 'undefined' && getSourceElement()) {\n      const observer = new MutationObserver(() => {\n        // Clear cache if source HTML changes\n        cachedParts = null;\n        log('Source mutated, cleared cache');\n      });\n      \n      observer.observe(getSourceElement(), {\n        childList: true,\n        subtree: true,\n        characterData: true\n      });\n    }\n  }\n\n  // Start the script\n  init();\n\n})();\n</script>"
